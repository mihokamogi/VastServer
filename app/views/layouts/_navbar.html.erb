            <a class="navbar-brand" href="/">DAC VAST Server</a>
          </div>
        
          <div class="collapse navbar-collapse" >
            <ul class="nav navbar-nav">
              <li class="<%= 'active' if current_page?(campaigns_path) %>"><%= link_to campaigns_path do %>Campaigns<% end %></li>
              <li class="<%= 'active' if current_page?(cuepoints_path) %>"><%= link_to cuepoints_path do %>Cuepoints<% end %></li>
              <li class="<%= 'active' if current_page?(results_path) %>"><%= link_to results_path do %>Results<% end %></li>
