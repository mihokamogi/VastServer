<div class="container">
  <div class="col-md-8">

  <h1>Results</h1>

<% if @results.any? %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>id</th>
        <th>campaign</th>
        <th>id</th>
        <th>cuepoint</th>
        <th>start</th>
        <th>complete</th>
      </tr>
    </thead>
    
    <tbody>
      <tr>  
      <% @results.each do |result| %>
        
          <td><%= campaign.id %> </td>
          <td><%= campaign.name %></td>
          <td><%= cuepoint.id %> </td>
          <td><%= cuepoint.name %></td>
          <td><%= result.count_start %></td>
          <td><%= result.count_end %></td>
          
      <% end %>
      </tr>
      
    </tbody>
  </table>
  <div>
    <%= paginate @campaigns %>
  </div>
  <% end %>
  </div>
</div>


