<div class="container">
  <div class="row">
    <div class="col-md-8">

    <h1>Results</h1>

      <% if @results.any? %>
        <table class="table table-striped">
          <thead>
            <tr>
              <th>id</th>
              <th>campaign</th>
              <th>id</th>
              <th>cuepoint</th>
              <th>start</th>
              <th>complete</th>
            </tr>
          </thead>
          
          <tbody>
            <% @results.each do |result| %>
              <tr>  
                <td><%= result.campaign.id %> </td>
                <td><%= result.campaign.name %></td>
                <td><%= result.cuepoint.id %> </td>
                <td><%= result.cuepoint.name %></td>
                <td><%= result.count_start %></td>
                <td><%= result.count_end %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
        <div>
          <%= paginate @results %>
        </div>
      <% end %>
    </div>
  </div>
</div>